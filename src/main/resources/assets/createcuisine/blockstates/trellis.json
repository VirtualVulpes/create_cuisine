{
  "multipart": [
    {
      "apply": {
        "model": "createcuisine:block/trellis_base"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_top"
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "createcuisine:block/trellis_bottom"
      },
      "when": {
        "down": "true"
      }
    }
  ]
}
